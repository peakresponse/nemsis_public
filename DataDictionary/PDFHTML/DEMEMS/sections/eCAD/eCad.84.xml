<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eCad.84" minOccurs="0" maxOccurs="1" isNillable="No">
<name>Incident Street Address</name>
<national>No</national>
<state>No</state>
<definition>?The street address where the patient was found, or, if no patient, the address to which the unit responded.?</definition>
<v2Number>E08_11</v2Number>
<refNumber>eScene.15</refNumber>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>This is a CMS standard.  According to the Medicare Claims Processing Manual, Chapter 15 - Ambulance, Ambulance suppliers bill using CMS-1500 form or CMS-1450 form for institution-based ambulance providers.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>55</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
  </constraints>
  <streetAddress>
    <constraints>
      <dataType>string</dataType>
      <constraint>
        <constraintTitle>minLength</constraintTitle>
        <constraintValue>1</constraintValue>
      </constraint>
      <constraint>
        <constraintTitle>maxLength</constraintTitle>
        <constraintValue>55</constraintValue>
      </constraint>
    </constraints>
  </streetAddress>
</restrictions>
</element>