<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eArrest.11" minOccurs="1" maxOccurs="1" isNillable="Yes">
<name>First Monitored Arrest Rhythm of the Patient</name>
<national>Yes</national>
<state>Yes</state>
<definition>Documentation of what the first monitored arrest rhythm which was noted</definition>
<v2Number>E11_05</v2Number>
<usage>Required</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>Added term "Arrest" to title to clarify this is the arrest rhythm<br/>Values "Other" and "Normal Sinus Rhythm" removed since they are not Cardiac Arrest Rhythms.<br/>Value "Ventricular Tachycardia" has been changed to "Ventricular Tachycardia-Pulseless".</v3Comment>
<v3Changes></v3Changes>
<performanceMeasuresGroup>
<performanceMeasure>Cardiac Arrest</performanceMeasure>
</performanceMeasuresGroup>
<restrictions>
      <enumerations>
        <choice>
           <code>3011001</code>
           <description>Asystole</description>
        </choice>
        <choice>
           <code>3011003</code>
           <description>Bradycardia</description>
        </choice>
        <choice>
           <code>3011005</code>
           <description>PEA</description>
        </choice>
        <choice>
           <code>3011007</code>
           <description>Unknown AED Non-Shockable Rhythm</description>
        </choice>
        <choice>
           <code>3011009</code>
           <description>Unknown AED Shockable Rhythm</description>
        </choice>
        <choice>
           <code>3011011</code>
           <description>Ventricular Fibrillation</description>
        </choice>
        <choice>
           <code>3011013</code>
           <description>Ventricular Tachycardia-Pulseless</description>
        </choice>
     </enumerations>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
	</notValues>
</restrictions>
</element>