<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="dAgency.07" minOccurs="1" maxOccurs="M" isNillable="Yes">
<name>EMS Agency Census Tracts</name>
<national>Yes</national>
<state>Yes</state>
<definition>The US census tracts in which the EMS agency formally provides service.</definition>
<v2Number></v2Number>
<usage>Required</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>This data element was added to better document the service area of the EMS Agency. Each state listed is associated with the counties, census tracts, and ZIP codes within the EMS Agency Service Area for each state.
													<br/><br/>Census Tract Data Website (files and descriptions): <a>http://www.census.gov/geo/www/2010census/tract_rel/tract_rel.html</a></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>15</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
	</notValues>
  <currentStatus>
    <choice><code>A</code><description>Active</description></choice>
    <choice><code>I</code><description>Inactive</description></choice>
	</currentStatus>
  <currentStatusDate>
    <constraints>
      <dataType>dateTime</dataType>
      <constraint>
        <constraintTitle>minInclusive</constraintTitle>
        <constraintValue>1950-01-01T00:00:00-00:00</constraintValue>
      </constraint>
      <constraint>
        <constraintTitle>maxInclusive</constraintTitle>
        <constraintValue>2050-01-01T00:00:00-00:00</constraintValue>
      </constraint>
      <constraint>
        <constraintTitle>pattern</constraintTitle>
        <constraintValue>[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\.\d+)?(\+|-)[0-9]{2}:[0-9]{2}</constraintValue>
      </constraint>
      </constraints>
  </currentStatusDate>
  <correlationID>Yes</correlationID>
</restrictions>
</element>