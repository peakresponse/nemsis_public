      <description>Governmental, Non-Fire</description>
    </choice>
    <choice>
      <code>9912005</code>
      <description>Hospital</description>
    </choice>
    <choice>
      <code>9912007</code>
      <description>Private, Nonhospital</description>
    </choice>
    <choice>
      <code>9912009</code>
      <description>Tribal</description>
    </choice>
  </enumerations>
</restrictions>
</element><element number="dAgency.14" minOccurs="1" maxOccurs="1" isNillable="No">
<name>EMS Agency Organizational Tax Status</name>
