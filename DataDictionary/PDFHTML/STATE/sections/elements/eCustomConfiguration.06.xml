<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eCustomConfiguration.06"  minOccurs="0"  maxOccurs="M"  dataTypeName="CustomValue"  isNillable="No">
<name>Custom Data Element Potential Values</name>
<national>No</national>
<deprecated></deprecated>
<state>No</state>
<definition>The values which are associated with the Custom Data Element. Values would be the choices provided to the user when they document the Custom Data Element</definition>
<v2Number></v2Number>
<usage>Optional</usage>
<performanceMeasuresGroup></performanceMeasuresGroup>
<v3Comment></v3Comment>
<v3Changes>Added to allow customized data elements to be inserted and collected from within the NEMSIS Version 3 standard.</v3Changes>
<deprecatedComment></deprecatedComment>
<restrictions>
  <CustomValue baseType="string">
    <facet name="minLength" value="1"/>
    <facet name="maxLength" value="100"/>
  </CustomValue>
</restrictions>
<attributes>
  <nemsisCode baseType="anySimpleType"><facet name="whiteSpace" value="preserve"/></nemsisCode>
  <customValueDescription baseType="anySimpleType"><facet name="whiteSpace" value="preserve"/></customValueDescription>
</attributes>
</element>
