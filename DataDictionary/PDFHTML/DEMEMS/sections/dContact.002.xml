<?xml-stylesheet type="text/xsl" href="../xslt/grouping.xslt"?>
<section name="dContact">
<group number="dContact.ContactInfoGroup" minOccurs="0" maxOccurs="M" correlationID="Yes" currentStatus="No" currentStatusDate="No">
<element number="dContact.01" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Agency Contact Type</name>
<national>No</national>
<state>Yes</state>
<definition>The contact type within the EMS agency.</definition>
<v2Number></v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <enumerations>
    <choice>
      <code>1101001</code>
      <description>Administrative Assistant</description>
    </choice>
    <choice>
      <code>1101003</code>
      <description>EMS Agency Director/Chief/Lead Administrator/CEO</description>
    </choice>
    <choice>
      <code>1101005</code>
      <description>EMS Assistant Agency Director/Chief/Administrator/CEO</description>
    </choice>
    <choice>
      <code>1101007</code>
      <description>EMS Assistant Medical Director</description>
    </choice>
    <choice>
      <code>1101009</code>
      <description>EMS IT/Data Specialist</description>
    </choice>
    <choice>
      <code>1101011</code>
      <description>EMS Medical Director</description>
    </choice>
    <choice>
      <code>1101013</code>
      <description>EMS Quality/Performance Improvement Specialist</description>
    </choice>
    <choice>
      <code>1101015</code>
      <description>EMS Training/Education Specialist</description>
    </choice>
    <choice>
      <code>1101017</code>
      <description>Other (Not Listed)</description>
    </choice>
  </enumerations>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="dContact.02" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Agency Contact Last Name</name>
<national>No</national>
<state>Yes</state>
<definition>The Last Name of the agency's primary contact.</definition>
<v2Number>D02_01</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>50</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="dContact.03" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Agency Contact First Name</name>
<national>No</national>
<state>Yes</state>
<definition>The first name of the agency's primary contact.</definition>
<v2Number>D02_03</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>50</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="dContact.04" minOccurs="0" maxOccurs="1" dataType="PersonMiddleName" isNillable="No">
<name>Agency Contact Middle Name/Initial</name>
<national>No</national>
<state>No</state>
<definition>The middle name or initial of the agency's primary contact.</definition>
<v2Number>D02_02</v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>50</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
  </constraints>
</restrictions>
</element><element number="dContact.05" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Agency Contact Address</name>
<national>No</national>
<state>Yes</state>
<definition>The street address of the agency contact's mailing address.</definition>
<v2Number>D02_04</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The address for the director/chief/lead administrator/CEO/ should be the main address for the EMS agency.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>55</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
  <streetAddress>
    <constraints>
      <dataType>string</dataType>
      <constraint>
        <constraintTitle>minLength</constraintTitle>
        <constraintValue>1</constraintValue>
      </constraint>
      <constraint>
        <constraintTitle>maxLength</constraintTitle>
        <constraintValue>55</constraintValue>
      </constraint>
    </constraints>
  </streetAddress>
</restrictions>
</element><element number="dContact.06" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Agency Contact City</name>
<national>No</national>
<state>Yes</state>
<definition>The city of the Agency contact's mailing address.</definition>
<v2Number>D02_05</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The address for the director/chief/lead administrator/CEO/ should be the main address for the EMS agency.<br/><br/>Based on GNIS Civil Code or Populated Place code.<br/><br/>GNIS Codes Website: <a>http://geonames.usgs.gov/domestic/download_data.htm</a></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>positiveInteger</dataType>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="dContact.07" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Agency Contact State</name>
<national>No</national>
<state>Yes</state>
<definition>The state of the Agency contact's mailing address.</definition>
<v2Number>D02_06</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The address for the director/chief/lead administrator/CEO/ should be the main address for the EMS agency. Based on the state ANSI Code. For individuals living outside the USA full address to be entered in the address line.
										<br/><br/>GNIS Codes Website: <a>http://geonames.usgs.gov/domestic/download_data.htm</a></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>length</constraintTitle>
      <constraintValue>2</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="dContact.08" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Agency Contact ZIP Code</name>
<national>No</national>
<state>Yes</state>
<definition>The ZIP code of the Agency's contact's mailing address.</definition>
<v2Number>D02_07</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The address for the director/chief/lead administrator/CEO/ should be the main address for the EMS agency.<br/><br/>Zip Codes Product Website: <a>https://www.zipcodedownload.com/Products/Product/Z5Commercial/Standard/Overview/</a>
<br/>Product: USA - 5-digit ZIP Code Database, Commercial Edition.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
      <patterns>
        <pattern>[0-9]{5}|[0-9]{5}-[0-9]{4}|[0-9]{5}-[0-9]{5}|[A-Z][0-9][A-Z] [0-9][A-Z][0-9]</pattern>
     </patterns>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="dContact.09" minOccurs="0" maxOccurs="1" dataType="ANSICountryCodesCAMXUS" isNillable="No">
<name>Agency Contact Country</name>
<national>No</national>
<state>No</state>
<definition>The Country code of the Agency's contact's mailing address.</definition>
<v2Number></v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The International Organization for Standardization (ISO) ISO 3166-1-alpha-2 code element for the country. The address for the director/chief/lead administrator/CEO/ should be the main address for the EMS agency. For individuals living outside the USA full address to be entered in the address line.
<br/><br/>ANSI Country Codes (ISO 3166): <a>http://www.iso.org/iso/country_codes/iso_3166_code_lists.htm</a></v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <enumerations>
    <choice>
      <code>CA</code>
      <description>Canada</description>
    </choice>
    <choice>
      <code>MX</code>
      <description>Mexico</description>
    </choice>
    <choice>
      <code>US</code>
      <description>United States</description>
    </choice>
  </enumerations>
</restrictions>
</element><element number="dContact.10" minOccurs="0" maxOccurs="M" isNillable="Yes">
<name>Agency Contact Phone Number</name>
<national>No</national>
<state>Yes</state>
<definition>Agency contact phone number.</definition>
<v2Number>D02_08</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes></v3Changes>
<restrictions>
      <patterns>
        <pattern>[2-9][0-9][0-9]-[2-9][0-9][0-9]-[0-9][0-9][0-9][0-9]</pattern>
     </patterns>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
  <correlationID>Yes</correlationID>
  <phoneNumberType>
    <choice><code>9913001</code><description>Fax</description></choice>
    <choice><code>9913003</code><description>Home</description></choice>
    <choice><code>9913005</code><description>Mobile</description></choice>
    <choice><code>9913007</code><description>Pager</description></choice>
    <choice><code>9913009</code><description>Work</description></choice>
  </phoneNumberType>
</restrictions>
</element><element number="dContact.11" minOccurs="0" maxOccurs="M" isNillable="Yes">
<name>Agency Contact Email Address</name>
<national>No</national>
<state>Yes</state>
<definition>The primary email address of the Agency contact.</definition>
<v2Number>D02_10</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The address for the director/chief/lead administrator/CEO/ should be the main address for the EMS agency.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
      <patterns>
        <pattern>.+@.+</pattern>
     </patterns>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>100</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>3</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
  <correlationID>Yes</correlationID>
  <emailAddressType>
    <choice><code>9904001</code><description>Personal</description></choice>
    <choice><code>9904003</code><description>Work</description></choice>
  </emailAddressType>
</restrictions>
</element><element number="dContact.12" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>EMS Agency Contact Web Address</name>
<national>No</national>
<state>Yes</state>
<definition>The primary website address of the agency.</definition>
<v2Number>D02_11</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The address for the director/chief/lead administrator/CEO/ should be the main address for the EMS agency. Web Address is not required for other contact types.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>255</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>3</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element></group>
