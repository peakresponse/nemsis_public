	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
  <correlationID>Yes</correlationID>
</restrictions>
</element><element number="dContact.15" minOccurs="0" maxOccurs="1" dataType="MedicalDirectorCompensation" isNillable="No">
<name>Medical Director Compensation</name>
<national>No</national>
<state>No</state>
<definition>Indication of Medical Director's Compensation.</definition>
<v2Number></v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>Associated with EMS Medical Directors only.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <enumerations>
    <choice>
      <code>1115001</code>
      <description>Compensated</description>
    </choice>
    <choice>
      <code>1115003</code>
      <description>Non-Compensated</description>
