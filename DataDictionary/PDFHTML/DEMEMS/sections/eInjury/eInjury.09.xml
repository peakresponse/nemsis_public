<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eInjury.09" minOccurs="0" maxOccurs="1" dataType="HeightOfFall" isNillable="No">
<name>Height of Fall (feet)</name>
<national>No</national>
<state>Yes</state>
<definition>The distance in feet the patient fell, measured from the lowest point of the patient to the ground</definition>
<v2Number>E10_10</v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>Classify same level falls as 0 feet</v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>integer</dataType>
    <constraint>
        <constraintTitle>minValue</constraintTitle>
      <constraintValue>0</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>maxValue</constraintTitle>
      <constraintValue>10000</constraintValue>
    </constraint>
  </constraints>
</restrictions>
</element>