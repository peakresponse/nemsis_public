<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eVitals.07" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>DBP (Diastolic Blood Pressure)</name>
<national>No</national>
<state>Yes</state>
<definition>The patient's diastolic blood pressure.</definition>
<v2Number>E14_05</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>Yes</pertinentNegative>
<v3Comment>Diastolic blood pressure pattern allows for the following values:<br/>1) A number 0 through 500 <br/>2) P<br/>3) p</v3Comment>
<v3Changes></v3Changes>
<restrictions>
	<pertinentNegatives>
		<choice><code>8801023</code><description>Unable to Complete</description>		</choice>
		<choice><code>8801005</code><description>Exam Finding Not Present</description>		</choice>
		<choice><code>8801019</code><description>Refused</description>		</choice>
	</pertinentNegatives>
      <patterns>
        <pattern>[5][0][0]|[1-4][0-9][0-9]|[0]|[1-9][0-9]|P|p</pattern>
     </patterns>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element>