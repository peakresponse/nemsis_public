<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eResponse.03" minOccurs="1" maxOccurs="1" isNillable="Yes">
<name>Incident Number</name>
<national>Yes</national>
<state>Yes</state>
<definition>The incident number assigned by the 911 Dispatch System</definition>
<v2Number>E02_02</v2Number>
<usage>Required</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>This number can be used to associate multiple EMS responses, dispatch information, and other information to the same EMS event or patient.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
      <constraints>
        <dataType>string</dataType>
        <constraint>
          <constraintTitle>maxLength</constraintTitle>
          <constraintValue>32</constraintValue>
        </constraint>
        <constraint>
          <constraintTitle>minLength</constraintTitle>
          <constraintValue>3</constraintValue>
        </constraint>
      </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
	</notValues>
</restrictions>
</element>