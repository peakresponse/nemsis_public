<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="dVehicle.13" minOccurs="0" maxOccurs="1" isNillable="No">
<name>Annual Vehicle Miles/Kilometers</name>
<national>No</national>
<state>No</state>
<definition>The number of miles/kilometers the vehicle accumulated in service for the agency for the defined year (Odometer Reading).</definition>
<v2Number>D06_10</v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>Associated with each year of (dVehicle.11) Year Miles/Kilometers Hours Accrued.</v3Comment>
<v3Changes></v3Changes>
<restrictions>
  <constraints>
    <dataType>decimal</dataType>
    <constraint>
        <constraintTitle>totalDigits</constraintTitle>
      <constraintValue>7</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>fractionDigits</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
  </constraints>
  <distanceUnit>
    <choice><code>9921001</code><description>Kilometers per Hour</description></choice>
    <choice><code>9921003</code><description>Miles per Hour</description></choice>
  </distanceUnit>
</restrictions>
</element>