											</xs:simpleType>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="eMedications.03" nillable="true" id="eMedications.eMedications.MedicationGiven">
