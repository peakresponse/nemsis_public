<?xml-stylesheet type="text/xsl" href="../xslt/grouping.xslt"?>
<section name="dDevice">
<group number="dDevice.DeviceGroup" minOccurs="0" maxOccurs="M" correlationID="Yes" currentStatus="Yes" currentStatusDate="Yes">
<element number="dDevice.01" minOccurs="0" maxOccurs="1" dataType="DeviceSerialNumber" isNillable="No">
<name>Medical Device Serial Number</name>
<national>No</national>
<state>No</state>
<definition>The unique manufacturer's serial number associated with a medical device.</definition>
<v2Number>D09_01</v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes></v3Changes>
<restrictions>
      <constraints>
        <dataType>string</dataType>
        <constraint>
          <constraintTitle>maxLength</constraintTitle>
          <constraintValue>50</constraintValue>
        </constraint>
        <constraint>
          <constraintTitle>minLength</constraintTitle>
          <constraintValue>2</constraintValue>
        </constraint>
      </constraints>
</restrictions>
</element><element number="dDevice.02" minOccurs="0" maxOccurs="1" dataType="DeviceNameorID" isNillable="No">
<name>Device Name or ID</name>
<national>No</national>
<state>No</state>
<definition>The local number or configurable Name/ID of the medical device.</definition>
<v2Number>D09_02</v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes></v3Changes>
<restrictions>
      <constraints>
        <dataType>string</dataType>
        <constraint>
          <constraintTitle>maxLength</constraintTitle>
          <constraintValue>50</constraintValue>
        </constraint>
        <constraint>
          <constraintTitle>minLength</constraintTitle>
          <constraintValue>2</constraintValue>
        </constraint>
      </constraints>
</restrictions>
</element><element number="dDevice.03" minOccurs="0" maxOccurs="M" isNillable="No">
