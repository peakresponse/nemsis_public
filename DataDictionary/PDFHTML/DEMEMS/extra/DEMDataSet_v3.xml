<?xml-stylesheet type="text/xsl" href="../xslt/root.xslt"?>
<section name="DEMDataset">
	<element number="dState" type="dState" minOccurs="0" maxOccurs="M">
		<name>State Required Demographic Data Elements</name>
	</element>
	<group number="dCustomConfiguration.CustomGroup" minOccurs="0" maxOccurs="M">
		<element number="dCustomConfiguration.01" minOccurs="0" maxOccurs="1" dataType="CustomTitle" isNillable="No">
			<name>Custom Data Element Title</name>
			<national>No</national>
			<state>No</state>
			<definition>This is the title of the custom data element created to collect information that is not defined formally in NEMSIS Version 3.</definition>
			<v2Number/>
			<usage>Optional</usage>
			<notValue>No</notValue>
			<pertinentNegative>No</pertinentNegative>
			<comment>This is grouped with all data elements in this section and can have multiple instances.</comment>
			<v3Changes>Added to allow customized data elements to be inserted and collected from within the NEMSIS Version 3 standard.</v3Changes>
			<restrictions>
				<constraints>
					<dataType>string</dataType>
					<constraint>
						<constraintTitle>maxLength</constraintTitle>
						<constraintValue>100</constraintValue>
					</constraint>
					<constraint>
						<constraintTitle>minLength</constraintTitle>
						<constraintValue>2</constraintValue>
					</constraint>
				</constraints>
			</restrictions>
		</element>
		<element number="dCustomConfiguration.02" minOccurs="0" maxOccurs="1" dataType="CustomDataType" isNillable="No">
			<name>Custom Definition</name>
			<national>No</national>
			<state>No</state>
			<definition/>
			<v2Number/>
			<usage>Optional</usage>
			<notValue>No</notValue>
			<pertinentNegative>No</pertinentNegative>
			<comment/>
			<v3Changes/>
			<restrictions>
				<enumerations>
					<choice>
						<code>9902001</code>
						<description>Binary</description>
					</choice>
					<choice>
						<code>9902003</code>
						<description>Date/Time</description>
					</choice>
					<choice>
						<code>9902005</code>
						<description>Integer/Number</description>
					</choice>
					<choice>
						<code>9902007</code>
						<description>Other (Not Listed)</description>
					</choice>
					<choice>
						<code>9902009</code>
						<description>Text/String</description>
					</choice>
				</enumerations>
			</restrictions>
		</element>
		<element number="dCustomConfiguration.03" minOccurs="0" maxOccurs="1" dataType="CustomDataType" isNillable="No">
			<name>EMS Custom Data Type</name>
			<national>No</national>
			<state>No</state>
			<definition/>
			<v2Number/>
			<usage>Optional</usage>
			<notValue>No</notValue>
			<pertinentNegative>No</pertinentNegative>
			<comment/>
			<v3Changes/>
			<restrictions>
				<enumerations>
					<choice>
						<code>9902001</code>
						<description>Binary</description>
					</choice>
					<choice>
						<code>9902003</code>
						<description>Date/Time</description>
					</choice>
					<choice>
						<code>9902005</code>
						<description>Integer/Number</description>
					</choice>
					<choice>
						<code>9902007</code>
						<description>Other (Not Listed)</description>
					</choice>
					<choice>
						<code>9902009</code>
						<description>Text/String</description>
					</choice>
				</enumerations>
			</restrictions>
		</element>
		<element number="dCustomConfiguration.04" minOccurs="0" maxOccurs="1" dataType="YesNoValues" isNillable="No">
			<name>EMS Custom Data Element Multiplicity</name>
			<national>No</national>
			<state>No</state>
			<definition>Indication if the data element will accept multiple values.</definition>
			<v2Number/>
			<usage>Optional</usage>
			<notValue>No</notValue>
			<pertinentNegative>No</pertinentNegative>
			<comment/>
			<v3Changes>Added to allow customized data elements to be inserted and collected from within the NEMSIS Version 3 standard.</v3Changes>
			<restrictions>
				<enumerations>
					<choice>
						<code>99223001</code>
						<description>No</description>
					</choice>
					<choice>
						<code>99223003</code>
						<description>Yes</description>
					</choice>
				</enumerations>
			</restrictions>
		</element>
		<element number="dCustomConfiguration.05" minOccurs="0" maxOccurs="1" dataType="ElementUsage" isNillable="No">
			<name>EMS Custom Data Element Usage</name>
			<national>No</national>
			<state>No</state>
			<definition>The Usage (Mandatory, Required, or Optional) for the EMS Custom Data Element.</definition>
			<v2Number/>
			<usage>Optional</usage>
			<notValue>No</notValue>
			<pertinentNegative>No</pertinentNegative>
			<comment>Mandatory = Must be completed and will not accept null values<br/>Required = Must be completed but will accept null values<br/>Optional = Not required but if collected, it cannot be a null value.</comment>
			<v3Changes>Added to allow customized data elements to be inserted and collected from within the NEMSIS Version 3 standard.</v3Changes>
			<restrictions>
				<enumerations>
					<choice>
						<code>9903001</code>
						<description>Mandatory</description>
					</choice>
					<choice>
						<code>9903003</code>
						<description>Required</description>
					</choice>
					<choice>
						<code>9903005</code>
						<description>Optional</description>
					</choice>
				</enumerations>
			</restrictions>
		</element>
		<element number="dCustomConfiguration.06" minOccurs="0" maxOccurs="M" dataType="CustomValue" isNillable="No">
			<name>EMS Custom Data Element Potential Values</name>
			<national>No</national>
			<state>No</state>
			<definition>The values which are associated with the Custom Data Element. Values would be the choices provided to the user when they document the Custom Data Element</definition>
			<v2Number/>
			<usage>Optional</usage>
			<notValue>No</notValue>
			<pertinentNegative>No</pertinentNegative>
			<comment/>
			<v3Changes>Added to allow customized data elements to be inserted and collected from within the NEMSIS Version 3 standard.</v3Changes>
			<restrictions>
				<constraints>
					<dataType>string</dataType>
					<constraint>
						<constraintTitle>maxLength</constraintTitle>
						<constraintValue>100</constraintValue>
					</constraint>
					<constraint>
						<constraintTitle>minLength</constraintTitle>
						<constraintValue>1</constraintValue>
					</constraint>
				</constraints>
			</restrictions>
		</element>
	</group>
	<group number="DemographicReport" minOccurs="1" maxOccurs="M">
		<element number="dAgency" minOccurs="1" maxOccurs="1">
			<name>Agency Information</name>
		</element>
		<element number="dContact" minOccurs="1"  maxOccurs="1">
			<name>Contact Information</name>
		</element>
		<element number="dConfiguration"  minOccurs="1" maxOccurs="1">
			<name>Configuration Information</name>
		</element>
		<element number="dLocation"  minOccurs="1"  maxOccurs="1">
			<name>Location Information</name>
		</element>
		<element number="dVehicle"  minOccurs="1"  maxOccurs="1">
			<name>Vehicle Information</name>
		</element>
		<element number="dPersonnel" minOccurs="0" maxOccurs="1">
			<name>Personnel Information</name>
		</element>
		<element number="dDevice" minOccurs="0" maxOccurs="1">
			<name>Device Information</name>
		</element>
		<element number="dFacility"  minOccurs="0" maxOccurs="1">
			<name>Device Information</name>
		</element>
		<element number="dCustom"  minOccurs="0" maxOccurs="1">
			<name>Custom Element Information</name>
		</element>
	</group>
</section>
