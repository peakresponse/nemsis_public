			<xs:enumeration value="07">
				<xs:annotation>
					<xs:documentation>Left against medical advice or discontinued care</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="09">
				<xs:annotation>
					<xs:documentation>Admitted as an inpatient to this hospital.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="20">
				<xs:annotation>
					<xs:documentation>Deceased/Expired (or did not recover - Religious Non Medical Health Care Patient)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="21">
				<xs:annotation>
					<xs:documentation>Discharged/transferred to court/law enforcement</xs:documentation>
