<?xml-stylesheet type="text/xsl" href="../xslt/grouping.xslt"?>
<section name="eCrew">
<group number="eCrew.CrewGroup" minOccurs="0" maxOccurs="M" correlationID="Yes" currentStatus="No" currentStatusDate="No">
<element number="eCrew.01" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Crew Member ID</name>
<national>No</national>
<state>Yes</state>
<definition>The state certification/licensure ID number assigned to the crew member.</definition>
<v2Number>E04_01</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>Document the state certification/licensure ID for the state where the event occurred.</v3Comment>
<v3Changes></v3Changes>
<performanceMeasuresGroup>
<performanceMeasure>Airway</performanceMeasure>
<performanceMeasure>Cardiac Arrest</performanceMeasure>
<performanceMeasure>Pediatric</performanceMeasure>
<performanceMeasure>Response</performanceMeasure>
<performanceMeasure>STEMI</performanceMeasure>
<performanceMeasure>Stroke</performanceMeasure>
<performanceMeasure>Trauma</performanceMeasure>
</performanceMeasuresGroup>
<restrictions>
  <constraints>
    <dataType>string</dataType>
    <constraint>
        <constraintTitle>maxLength</constraintTitle>
      <constraintValue>15</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>minLength</constraintTitle>
      <constraintValue>2</constraintValue>
    </constraint>
  </constraints>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="eCrew.02" minOccurs="0" maxOccurs="1" isNillable="Yes">
<name>Crew Member Level</name>
<national>No</national>
<state>Yes</state>
<definition>The functioning level of the crew member ID during this EMS patient encounter.</definition>
<v2Number>E04_03</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>The category EMT-Intermediate includes EMS professionals with an “85” or “99” certification level.</v3Comment>
<v3Changes></v3Changes>
<performanceMeasuresGroup>
<performanceMeasure>Airway</performanceMeasure>
<performanceMeasure>Cardiac Arrest</performanceMeasure>
<performanceMeasure>Pediatric</performanceMeasure>
<performanceMeasure>Response</performanceMeasure>
<performanceMeasure>STEMI</performanceMeasure>
<performanceMeasure>Stroke</performanceMeasure>
<performanceMeasure>Trauma</performanceMeasure>
</performanceMeasuresGroup>
<restrictions>
  <enumerations>
    <choice>
      <code>2302001</code>
      <description>2009 Advanced Emergency Medical Technician (AEMT)</description>
    </choice>
    <choice>
      <code>2302003</code>
      <description>2009 Emergency Medical Responder (EMR)</description>
    </choice>
    <choice>
      <code>2302005</code>
      <description>2009 Emergency Medical Technician (EMT)</description>
    </choice>
    <choice>
      <code>2302007</code>
      <description>2009 Paramedic</description>
    </choice>
    <choice>
      <code>2302009</code>
      <description>First Responder</description>
    </choice>
    <choice>
      <code>2302011</code>
      <description>EMT-Basic</description>
    </choice>
    <choice>
      <code>2302013</code>
      <description>EMT-Intermediate</description>
    </choice>
    <choice>
      <code>2302015</code>
      <description>EMT-Paramedic</description>
    </choice>
    <choice>
      <code>2302017</code>
      <description>Nurse</description>
    </choice>
    <choice>
      <code>2302019</code>
      <description>Other Healthcare Professional</description>
    </choice>
    <choice>
      <code>2302021</code>
      <description>Other Non-Healthcare Professional</description>
    </choice>
    <choice>
      <code>2302023</code>
      <description>Physician</description>
    </choice>
    <choice>
      <code>2302025</code>
      <description>Respiratory Therapist</description>
    </choice>
    <choice>
      <code>2302027</code>
      <description>Student</description>
    </choice>
    <choice>
      <code>2302029</code>
      <description>Critical Care Paramedic</description>
    </choice>
    <choice>
      <code>2302031</code>
      <description>Community Paramedicine</description>
    </choice>
  </enumerations>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
</restrictions>
</element><element number="eCrew.03" minOccurs="0" maxOccurs="M" isNillable="Yes">
<name>Crew Member Response Role</name>
<national>No</national>
<state>Yes</state>
<definition>The role(s) of the role member during response, at scene treatment, and/or transport.</definition>
<v2Number>E04_02</v2Number>
<usage>Recommended</usage>
<notValue>Yes</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment>This element has been changed to allow for multiple selections. There must be a driver for response and transport. There must be a primary patient caregiver for scene and transport.</v3Comment>
<v3Changes></v3Changes>
<performanceMeasuresGroup>
<performanceMeasure>Airway</performanceMeasure>
<performanceMeasure>Cardiac Arrest</performanceMeasure>
<performanceMeasure>Pediatric</performanceMeasure>
<performanceMeasure>Response</performanceMeasure>
<performanceMeasure>STEMI</performanceMeasure>
<performanceMeasure>Stroke</performanceMeasure>
<performanceMeasure>Trauma</performanceMeasure>
</performanceMeasuresGroup>
<restrictions>
  <enumerations>
    <choice>
      <code>2303001</code>
      <description>Driver/Pilot-Response</description>
    </choice>
    <choice>
      <code>2303003</code>
      <description>Driver/Pilot-Transport</description>
    </choice>
    <choice>
      <code>2303005</code>
      <description>Other (Not Listed)</description>
    </choice>
    <choice>
      <code>2303007</code>
      <description>Other Patient Caregiver-At Scene</description>
    </choice>
    <choice>
      <code>2303009</code>
      <description>Other Patient Caregiver-Transport</description>
    </choice>
    <choice>
      <code>2303011</code>
      <description>Primary Patient Caregiver-At Scene</description>
    </choice>
    <choice>
      <code>2303013</code>
      <description>Primary Patient Caregiver-Transport</description>
    </choice>
  </enumerations>
	<notValues>
		<choice><code>7701003</code><description>Not Recorded</description>		</choice>
		<choice><code>7701001</code><description>Not Applicable</description>		</choice>
		<choice><code>7701005</code><description>Not Reporting</description>		</choice>
	</notValues>
  <correlationID>Yes</correlationID>
</restrictions>
</element></group>
</section>
