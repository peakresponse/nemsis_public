<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eOutcome.14" minOccurs="0" maxOccurs="1" dataType="LengthOfStay" isNillable="No">
<name>Total ICU Length of Stay</name>
<national>No</national>
<state>No</state>
<definition>The total number of patient days in any ICU (including all ICU episodes).</definition>
<v2Number></v2Number>
<usage>Optional</usage>
<notValue>No</notValue>
<pertinentNegative>No</pertinentNegative>
<v3Comment></v3Comment>
<v3Changes>Added to better measure patient outcomes.</v3Changes>
<restrictions>
  <constraints>
    <dataType>integer</dataType>
    <constraint>
        <constraintTitle>minValue</constraintTitle>
      <constraintValue>1</constraintValue>
    </constraint>
    <constraint>
        <constraintTitle>maxValue</constraintTitle>
      <constraintValue>400</constraintValue>
    </constraint>
  </constraints>
</restrictions>
</element>