<?xml-stylesheet type="text/xsl" href="../../xslt/element.xslt"?>
<element number="eDisposition.21"  minOccurs="1"  maxOccurs="1"  dataTypeName="TypeOfDestination"  isNillable="Yes">
<name>Type of Destination</name>
<national>Yes</national>
<deprecated></deprecated>
<state>Yes</state>
<definition>The type of destination the patient was delivered or transferred to.</definition>
<v2Number>E20_17</v2Number>
<usage>Required</usage>
<performanceMeasuresGroup><performanceMeasure>Airway</performanceMeasure><performanceMeasure>Cardiac Arrest</performanceMeasure><performanceMeasure>Pediatric</performanceMeasure><performanceMeasure>Response</performanceMeasure><performanceMeasure>STEMI</performanceMeasure><performanceMeasure>Stroke</performanceMeasure><performanceMeasure>Trauma</performanceMeasure></performanceMeasuresGroup>
<v3Comment>Required if Patient Transported by EMS.</v3Comment>
<v3Changes></v3Changes>
<deprecatedComment></deprecatedComment>
<restrictions>
  <TypeOfDestination baseType="string">
    <facet name="enumeration" value="4221001" annotation="Home"/>
    <facet name="enumeration" value="4221003" annotation="Hospital-Emergency Department"/>
    <facet name="enumeration" value="4221005" annotation="Hospital-Non-Emergency Department Bed"/>
    <facet name="enumeration" value="4221007" annotation="Medical Office/Clinic"/>
    <facet name="enumeration" value="4221009" annotation="Morgue/Mortuary"/>
    <facet name="enumeration" value="4221011" annotation="Nursing Home/Assisted Living Facility"/>
    <facet name="enumeration" value="4221013" annotation="Other"/>
    <facet name="enumeration" value="4221015" annotation="Other EMS Responder (air)"/>
    <facet name="enumeration" value="4221017" annotation="Other EMS Responder (ground)"/>
    <facet name="enumeration" value="4221019" annotation="Police/Jail"/>
    <facet name="enumeration" value="4221021" annotation="Urgent Care"/>
    <facet name="enumeration" value="4221023" annotation="Freestanding Emergency Department"/>
  </TypeOfDestination>
</restrictions>
<attributes>
  <NV>
    <NV.NotApplicable><facet name="enumeration" value="7701001" annotation="Not Applicable"/></NV.NotApplicable>
    <NV.NotRecorded><facet name="enumeration" value="7701003" annotation="Not Recorded"/></NV.NotRecorded>
  </NV>
</attributes>
<asserts>
  <assert id="nemSch_consistency_eDisposition.21_hospital_eDisposition.22" role="Warning">
      <elementRef elementNumber="eDisposition.22">Hospital In-Patient Destination</elementRef> should be recorded when <elementRef elementNumber="eDisposition.21">Type of Destination</elementRef> is "Hospital-Non-Emergency Department Bed".
      </assert>
  <assert id="nemSch_consistency_eDisposition.21_hospital_eDisposition.23_cardiacarrest" role="Warning">
      <elementRef elementNumber="eDisposition.23">Hospital Capability</elementRef> should be recorded when <elementRef elementNumber="eDisposition.21">Type of Destination</elementRef> is "Hospital..." or "Freestanding Emergency Department" and <elementRef elementNumber="eArrest.01">Cardiac Arrest</elementRef> is "Yes...".
      </assert>
  <assert id="nemSch_consistency_eDisposition.21_hospital_eDisposition.23_stroke" role="Warning">
      <elementRef elementNumber="eDisposition.23">Hospital Capability</elementRef> should be recorded when <elementRef elementNumber="eDisposition.21">Type of Destination</elementRef> is "Hospital..." or "Freestanding Emergency Department" and <elementRef elementNumber="eVitals.29">Stroke Scale Score</elementRef> is "Positive".
      </assert>
  <assert id="nemSch_consistency_eDisposition.21_hospital_eDisposition.23_stemi" role="Warning">
      <elementRef elementNumber="eDisposition.23">Hospital Capability</elementRef> should be recorded when <elementRef elementNumber="eDisposition.21">Type of Destination</elementRef> is "Hospital..." or "Freestanding Emergency Department" and <elementRef elementNumber="eVitals.03">Cardiac Rhythm / Electrocardiography (ECG)</elementRef> is "STEMI...".
      </assert>
  <assert id="nemSch_consistency_eDisposition.12_all_warning_eDisposition.21" role="Warning">
      <elementRef elementNumber="eDisposition.21">Type of Destination</elementRef> should be recorded when <elementRef elementNumber="eDisposition.12">Incident/Patient Disposition</elementRef> is "... (With Transport)" or "Patient Treated, Transported by this EMS Unit".
      </assert>
  <assert id="nemSch_consistency_eNilNvPn_Nv_Nil" role="Error">
      When Type of Destination has a Not Value (Not Applicable, Not Recorded, or Not Reporting), it should be empty.
      </assert>
  <assert id="nemSch_consistency_eNilNvPn_Nil_Nv" role="Error">
      When Type of Destination is empty, it should have a Not Value (Not Applicable, Not Recorded, or Not Reporting, if allowed for the element) or a Pertinent Negative (if allowed for the element), or it should be omitted (if the element is optional).
      </assert>
</asserts>
</element>
